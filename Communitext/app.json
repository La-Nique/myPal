{
  "expo": {
    "name": "Communitext",
    "slug": "Communitext",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F4E4D5",
      "borderRadius": "20"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#F4E4D5"
      },
      "permissions": [
        "WRITE_SETTINGS"
      ],
      "package": "com.abidh0601.Communitext"
    },
    "web": {
      "favicon": "./assets/icon.png"
    }
  }
}
