{
  "expo": {
    "name": "Communitext",
    "slug": "Communitext",
    "version": "4.0.17",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F4E4D5",
      "borderRadius": "20"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#F4E4D5"
      }
    },
    "web": {
      "favicon": "./assets/icon.png"
    }
  }
}
